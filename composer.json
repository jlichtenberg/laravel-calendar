{
    "name": "jlichtenberg/laravel-calendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "jlichtenberg",
            "email": "lichtenberg.joel@gmail.com"
        }
    ],
    "require": {
        "php": ">7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "jlichtenberg\\LaravelCalendar\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jlichtenberg\\LaravelCalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "jlichtenberg\\LaravelCalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
